<div class="block">
  ember-cli-couch
</div>
<div class="block">
  <div>Database: {{model.database.db_name}}</div>
  <div>URL: {{db.url}}</div>
  <div>CouchDB: v{{model.couch.version}}</div>
  <div>Session:
    {{#with model.session.userCtx.name as |name|}}
      {{#if name}}
        {{name}}
      {{else}}
        Not logged in
      {{/if}}
    {{/with}}
  </div>
</div>
<div class="block">
  <div>{{db}}</div>
  <div>{{db.couch}}</div>
  <div>{{db.couch.couches}}</div>
</div>
<div class="block">
  <div>{{model.message._id}} / {{model.message._rev}}</div>
  <div class='strong'>{{model.message.message}}</div>
  <div>â€” {{model.message.author}}</div>
</div>
<div class="block">
  <div>{{changes.changes}}</div>
  {{#each changes.log as |item|}}
    {{ui-json data=item}}
  {{/each}}
</div>
